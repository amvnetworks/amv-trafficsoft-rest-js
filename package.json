{
  "name": "amv-trafficsoft-rest-js",
  "version": "1.0.0",
  "description": "",
  "main": "dist/amv-trafficsoft-rest-js.js",
  "scripts": {
    "precommit": "npm test && npm run build",
    "build": "rimraf dist && webpack --no-minimize && webpack",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover node_modules/mocha/bin/_mocha --report lcovonly -- -R spec",
    "coveralls": "npm run coverage && coveralls < coverage/lcov.info && rimraf coverage",
    "test": "npm run test-server && npm run test-browser",
    "test-server": "mocha test/server --recursive --bail --require babel-core/register mocha.config.js",
    "test-browser": "karma start --single-run",
    "tdd": "npm run test-server -- --watch",
    "tdd-browser": "karma start"
  },
  "repository": "amvnetworks/amv-trafficsoft-rest-js",
  "keywords": [],
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.4",
    "babel-loader": "^6.2.2",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.2.0",
    "coveralls": "^2.11.4",
    "eslint": "^1.4.1",
    "eslint-plugin-babel": "^3.1.0",
    "husky": "^0.10.1",
    "isparta": "^4.0.0",
    "karma": "^0.13.14",
    "karma-chrome-launcher": "^0.2.1",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.1",
    "karma-sinon-chai": "^1.1.0",
    "karma-spec-reporter": "0.0.23",
    "karma-webpack": "^1.5.0",
    "mocha": "^2.3.3",
    "mocha-lcov-reporter": "^1.0.0",
    "rimraf": "^2.4.3",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.12.2"
  },
  "author": {
    "name": "Alois Leitner",
    "email": "alois.leitner@amv-networks.com"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.1",
    "lodash": "^4.17.4"
  }
}
